{
    "name": "SocketXP - Remote Access",
    "version": "1.0.1",
    "slug": "socketxp",
    "description": "Secure remote access without public IP or configuring NAT. Account registration required.",
    "url": "https://www.socketxp.com/documentation",
    "arch": ["armhf", "aarch64", "amd64"],
    "startup": "application",
    "boot": "auto",
    "host_network": true,
    "map": ["ssl:rw"],
    "image": "expresssocket/home-assistant-{arch}",
    "options": {
        "authtoken": "null",
        "tunnel_enabled": true,
        "tunnels" : [
            {
                "destination": "https://127.0.0.1:8123",
                "protocol": "tls",
                "custom_domain": "ha.example.com"
            }
        ],
        "relay_enabled": false, 
        "relays": [
            {
                "destination": "https://127.0.0.1:8123"
            }
        ]
    },
    "schema": {
        "authtoken": "str",
        "tunnel_enabled": "bool",
        "tunnels" : [
            {
                "destination": "url",
                "protocol": "str?",
                "custom_domain": "str?"
            }
        ],
        "relay_enabled": "bool",
        "relays": [
            {
                "destination": "url"
            }
        ]
    }
}

